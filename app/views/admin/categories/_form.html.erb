<div class="panel-body">
  <%= form_for [:admin, @category] do |f| %>
    <div class="row">
      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: "form-control" %>
      </div>
    </div>
    <%= f.button "Submit", class: "btn btn-primary" %>
  <% end %>
</div>